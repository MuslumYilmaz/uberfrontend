{
    "dependencies": {
        "vue": "3.x"
    },
    "openFile": "/src/App.vue",
    "files": {
        "package.json": "{\n  \"name\": \"vue-contact-form\",\n  \"version\": \"0.1.0\",\n  \"private\": true,\n  \"dependencies\": {\n    \"vue\": \"3.x\"\n  },\n  \"scripts\": {\n    \"serve\": \"vue-cli-service serve\"\n  }\n}\n",
        "public/index.html": "<!doctype html><html lang=\"en\"><head><meta charset=\"utf-8\"><meta name=\"viewport\" content=\"width=device-width,initial-scale=1\"><title>Vue SDK • Contact Form</title></head><body><div id=\"app\"></div></body></html>",
        "src/main.ts": "import { createApp } from 'vue';\nimport App from './App.vue';\nimport './App.css';\n\ncreateApp(App).mount('#app');\n",
        "src/App.vue": "<template>\n  <main class=\"main\">\n    <section class=\"section\">\n      <h1>Contact Us</h1>\n\n      <form class=\"panel\" novalidate @submit=\"handleSubmit\">\n        <div class=\"field\">\n          <label for=\"name\">Name</label>\n          <input\n            id=\"name\"\n            type=\"text\"\n            v-model=\"name\"\n            @blur=\"touchedName = true\"\n          />\n          <!-- TODO: Show a required error message when name is empty & touched -->\n        </div>\n\n        <div class=\"field\">\n          <label for=\"email\">Email</label>\n          <input\n            id=\"email\"\n            type=\"email\"\n            v-model=\"email\"\n            @blur=\"touchedEmail = true\"\n          />\n          <!-- TODO: Show required + invalid email messages -->\n        </div>\n\n        <div class=\"field\">\n          <label for=\"message\">Message</label>\n          <textarea\n            id=\"message\"\n            rows=\"4\"\n            v-model=\"message\"\n            @blur=\"touchedMessage = true\"\n          />\n          <!-- TODO: Show a required / minimum-length error message -->\n        </div>\n\n        <button\n          type=\"submit\"\n          class=\"primary\"\n          :disabled=\"!formIsValid || isSubmitting\"\n        >\n          {{ isSubmitting ? 'Sending…' : 'Send message' }}\n        </button>\n\n        <p v-if=\"successMessage\" class=\"success\">{{ successMessage }}</p>\n        <p v-if=\"errorMessage\" class=\"error\">{{ errorMessage }}</p>\n      </form>\n    </section>\n  </main>\n</template>\n\n<script setup lang=\"ts\">\nimport { ref, computed } from 'vue';\n\n// Form field state (TODO: wire these up with validation)\nconst name = ref('');\nconst email = ref('');\nconst message = ref('');\n\n// Touched flags for showing validation messages\nconst touchedName = ref(false);\nconst touchedEmail = ref(false);\nconst touchedMessage = ref(false);\n\n// Submission state + feedback messages\nconst isSubmitting = ref(false);\nconst successMessage = ref('');\nconst errorMessage = ref('');\n\n// TODO: Replace these with real validity checks\n// (all fields required + basic email + min length for message)\nconst nameIsValid = computed(() => false);\nconst emailIsValid = computed(() => false);\nconst messageIsValid = computed(() => false);\n\nconst formIsValid = computed(() => false);\n\nconst handleSubmit = (event: any) => {\n  event.preventDefault();\n\n  // TODO: Implement the full submit logic\n  // - Mark all fields as touched\n  // - Validate values and bail out if invalid\n  // - If valid:\n  //   - Clear previous success/error messages\n  //   - Set isSubmitting.value = true\n  //   - Send a POST request with { name, email, message } to\n  //     https://jsonplaceholder.typicode.com/posts\n  //   - On success, show \"Your message has been sent. Thank you!\" and\n  //     optionally reset the form\n  //   - On error, show \"Something went wrong. Please try again.\"\n\n  console.log('submit() called with:', {\n    name: name.value,\n    email: email.value,\n    message: message.value\n  });\n};\n</script>\n",
        "src/App.css": ":root { color-scheme: dark; }\nhtml, body { margin: 0; height: 100%; background:#020617; font-family: system-ui, -apple-system, Segoe UI, Roboto, Helvetica, Arial, sans-serif; }\n\n.main { min-height:100vh; background:#020617; color:#e5e7eb; display:grid; place-items:center; padding:24px; }\n.section { width:100%; max-width:480px; display:grid; gap:16px; }\nh1 { margin:0; font-size:20px; font-weight:700; }\n\n.panel { background:#020617; border:1px solid #1f2937; border-radius:12px; padding:16px; display:grid; gap:12px; }\n.field { display:grid; gap:4px; }\nlabel { font-size:13px; color:#9ca3af; }\ninput, textarea { background:#020617; border-radius:8px; border:1px solid #1f2937; padding:8px 10px; color:#e5e7eb; font-size:14px; }\ninput:focus, textarea:focus { outline:none; border-color:#6366f1; box-shadow:0 0 0 1px #6366f1; }\nbutton.primary { margin-top:4px; background:#4ade80; color:#022c22; border:none; border-radius:999px; padding:10px 16px; font-weight:600; font-size:14px; cursor:pointer; display:inline-flex; align-items:center; justify-content:center; }\nbutton.primary[disabled] { opacity:.6; cursor:not-allowed; }\n.success { margin:4px 0 0; font-size:13px; color:#22c55e; }\n.error { margin:4px 0 0; font-size:13px; color:#f97316; }\n",
        "tsconfig.json": "{\n  \"compilerOptions\": {\n    \"target\": \"ES2020\",\n    \"module\": \"ESNext\",\n    \"moduleResolution\": \"Node\",\n    \"strict\": true,\n    \"allowJs\": true\n  }\n}\n"
    }
}