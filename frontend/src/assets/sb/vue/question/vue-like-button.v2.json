        "package.json": "{\n  \"name\": \"vue-like-button\",\n  \"version\": \"0.1.0\",\n  \"private\": true,\n  \"dependencies\": {\n    \"vue\": \"3.x\"\n  },\n  \"scripts\": {\n    \"serve\": \"vue-cli-service serve\"\n  }\n}\n",
        "public/index.html": "<!doctype html><html lang=\"en\"><head><meta charset=\"utf-8\"><meta name=\"viewport\" content=\"width=device-width,initial-scale=1\"><title>Vue SDK ‚Ä¢ Like Button</title></head><body><div id=\"app\"></div></body></html>",
        "src/main.ts": "import { createApp } from 'vue';\nimport App from './App.vue';\nimport './App.css';\n\ncreateApp(App).mount('#app');\n",
        "src/App.vue": "<template>\n  <main class=\"page\">\n    <section class=\"card\">\n      <h1 class=\"title\">Like Button</h1>\n      <p class=\"subtitle\">Build a Facebook-style like button that toggles state and updates a counter.</p>\n\n      <div class=\"row\">\n        <button\n          class=\"likeBtn\"\n          :class=\"{ active: liked }\"\n          @click=\"toggleLike\"\n          :aria-pressed=\"liked\"\n          aria-label=\"like button\"\n        >\n          <span class=\"icon\" aria-hidden=\"true\">üëç</span>\n          <span class=\"label\">{{ liked ? 'Liked' : 'Like' }}</span>\n        </button>\n\n        <div class=\"count\" aria-label=\"like count\">{{ count }}</div>\n      </div>\n\n      <p class=\"hint\">\n        Requirements: clicking toggles the button style and label, and updates the counter (+1 / -1). Count must never go below 0.\n      </p>\n    </section>\n  </main>\n</template>\n\n<script setup lang=\"ts\">\nimport { ref } from 'vue';\n\n// TODO: Track like state\nconst liked = ref(false);\n\n// TODO: Track like count\nconst count = ref(120);\n\n// TODO: Implement toggle behavior\n// - When liking: liked=true and count++\n// - When unliking: liked=false and count-- (but never below 0)\nconst toggleLike = () => {\n  // Starter intentionally does nothing\n};\n</script>\n",
        "src/App.css": ":root{color-scheme:dark;}html,body{margin:0;height:100%;background:#0a0a0a;}body{font-family:system-ui,-apple-system,Segoe UI,Roboto,Helvetica,Arial,sans-serif;}\n.page{min-height:100vh;display:grid;place-items:center;padding:24px;background:#0a0a0a;color:#e5e7eb;}\n.card{width:100%;max-width:520px;background:#0b1020;border:1px solid #1f2937;border-radius:14px;padding:18px;display:grid;gap:14px;}\n.title{margin:0;font-size:18px;font-weight:800;}\n.subtitle{margin:0;color:#9ca3af;font-size:12px;}\n.row{display:flex;align-items:center;gap:12px;}\n.likeBtn{display:inline-flex;align-items:center;gap:10px;border-radius:999px;padding:10px 14px;border:1px solid #374151;background:#0a0f1e;color:#e5e7eb;font-weight:800;cursor:pointer;}\n.likeBtn:hover{border-color:#4b5563;}\n.likeBtn.active{background:rgba(59,130,246,0.15);border-color:rgba(59,130,246,0.7);color:#dbeafe;}\n.icon{font-size:16px;line-height:1;}\n.label{font-size:14px;}\n.count{min-width:64px;text-align:center;background:#0a0f1e;border:1px solid #1f2937;border-radius:12px;padding:10px 12px;font-weight:900;}\n.hint{margin:0;color:#9ca3af;font-size:12px;line-height:1.35;}\n",
        "tsconfig.json": "{\n  \"compilerOptions\": {\n    \"target\": \"ES2020\",\n    \"module\": \"ESNext\",\n    \"moduleResolution\": \"Node\",\n    \"strict\": true,\n    \"allowJs\": true\n  }\n}\n"
    }
