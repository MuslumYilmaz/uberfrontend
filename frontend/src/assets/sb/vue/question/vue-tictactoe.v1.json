        "package.json": "{\n  \"name\": \"vue-tictactoe\",\n  \"version\": \"0.1.0\",\n  \"private\": true,\n  \"dependencies\": {\n    \"vue\": \"3.x\"\n  },\n  \"scripts\": {\n    \"serve\": \"vue-cli-service serve\"\n  }\n}\n",
        "public/index.html": "<!doctype html><html lang=\"en\"><head><meta charset=\"utf-8\"><meta name=\"viewport\" content=\"width=device-width,initial-scale=1\"><title>Vue SDK â€¢ Tic-Tac-Toe</title></head><body><div id=\"app\"></div></body></html>",
        "src/main.ts": "import { createApp } from 'vue';\nimport App from './App.vue';\nimport './App.css';\n\ncreateApp(App).mount('#app');\n",
        "src/App.vue": "<template>\n  <main class=\"page\">\n    <section class=\"card\">\n      <h1 class=\"title\">Tic-Tac-Toe</h1>\n      <p class=\"subtitle\">Two players take turns. First to 3 in a row wins.</p>\n\n      <div class=\"status\" aria-live=\"polite\">{{ status }}</div>\n\n      <div class=\"board\" role=\"grid\" aria-label=\"tic tac toe board\">\n        <button\n          v-for=\"(cell, i) in board\"\n          :key=\"i\"\n          class=\"cell\"\n          type=\"button\"\n          @click=\"play(i)\"\n          :disabled=\"isGameOver || cell !== null\"\n        >\n          {{ cell ?? '' }}\n        </button>\n      </div>\n\n      <div class=\"actions\">\n        <button class=\"ghost\" type=\"button\" @click=\"reset\">Reset</button>\n      </div>\n    </section>\n  </main>\n</template>\n\n<script setup lang=\"ts\">\nimport { computed, ref } from 'vue';\n\ntype Cell = 'X' | 'O' | null;\n\nconst EMPTY_BOARD: Cell[] = Array.from({ length: 9 }, () => null);\n\n// TODO: check winner based on winning line indices\nfunction calculateWinner(b: Cell[]): 'X' | 'O' | null {\n  return null;\n}\n\n// TODO: store board\nconst board = ref<Cell[]>([...EMPTY_BOARD]);\n\n// TODO: current player starts as 'X'\nconst currentPlayer = ref<'X' | 'O'>('X');\n\n// TODO: derived: winner\nconst winner = computed(() => null as 'X' | 'O' | null);\n\n// TODO: derived: draw when full and no winner\nconst isDraw = computed(() => false);\n\n// TODO: derived: game over\nconst isGameOver = computed(() => false);\n\n// TODO: derived: status text\nconst status = computed(() => 'Next: X');\n\n// TODO:\n// - ignore if game over\n// - ignore if cell already filled\n// - place current player's mark\n// - if winner or draw -> stop\n// - else switch player\nconst play = (i: number) => {};\n\n// TODO: reset board, current player\nconst reset = () => {};\n</script>\n",
        "src/App.css": ":root{color-scheme:dark;}html,body{margin:0;height:100%;background:#0a0a0a;}body{font-family:system-ui,-apple-system,Segoe UI,Roboto,Helvetica,Arial,sans-serif;}\n.page{min-height:100vh;display:grid;place-items:center;padding:24px;background:#0a0a0a;color:#e5e7eb;}\n.card{width:100%;max-width:520px;background:#0b1020;border:1px solid #1f2937;border-radius:14px;padding:18px;display:grid;gap:14px;}\n.title{margin:0;font-size:18px;font-weight:800;}\n.subtitle{margin:0;color:#9ca3af;font-size:12px;}\n.status{padding:10px 12px;border:1px solid #1f2937;border-radius:12px;background:#0a0f1e;font-weight:700;}\n.board{display:grid;grid-template-columns:repeat(3,1fr);gap:10px;}\n.cell{aspect-ratio:1/1;border-radius:14px;border:1px solid rgba(31,41,55,.7);background:#0a0f1e;color:#e5e7eb;font-size:34px;font-weight:900;cursor:pointer;display:grid;place-items:center;}\n.cell:disabled{opacity:.55;cursor:not-allowed;}\n.actions{display:flex;justify-content:flex-end;}\n.ghost{background:transparent;color:#e5e7eb;border:1px solid #374151;border-radius:12px;padding:10px 14px;font-weight:800;cursor:pointer;}\n",
        "tsconfig.json": "{\n  \"compilerOptions\": {\n    \"target\": \"ES2020\",\n    \"module\": \"ESNext\",\n    \"moduleResolution\": \"Node\",\n    \"strict\": true,\n    \"allowJs\": true\n  }\n}\n"
