{
  "name": "frontendatlas",
  "version": "0.0.0",
  "scripts": {
    "gen:data": "node scripts/gen-data-version.mjs && node scripts/generate-sitemap.mjs",
    "lint:tags": "node scripts/lint-tags.mjs",
    "lint:tags:fix": "node scripts/lint-tags.mjs --fix",
    "lint:topics": "node scripts/lint-topics.mjs",
    "lint:topics:fix": "node scripts/lint-topics.mjs --fix",
    "lint:tracks": "node scripts/lint-tracks.mjs",
    "lint:questions": "node scripts/lint-questions.mjs",
    "lint:questions:fix": "node scripts/lint-questions.mjs --fix",
    "start:e2e": "npm run gen:data && ng serve --configuration development --port 4200 --host 127.0.0.1",
    "start:prod": "npm run gen:data && ng serve --configuration production",
    "build": "npm run gen:data && ng build --configuration production",
    "test": "ng test --watch=false --browsers=ChromeHeadless && node scripts/sitemap.test.mjs",
    "test:e2e": "playwright test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.12",
    "@angular/common": "17.3.12",
    "@angular/compiler": "17.3.12",
    "@angular/core": "17.3.12",
    "@angular/forms": "17.3.12",
    "@angular/platform-browser": "17.3.12",
    "@angular/platform-browser-dynamic": "17.3.12",
    "@angular/router": "17.3.12",
    "@primeuix/themes": "^1.2.1",
    "canvas-confetti": "^1.9.3",
    "dompurify": "^3.3.1",
    "esbuild-wasm": "^0.25.10",
    "localforage": "^1.10.0",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^17.18.15",
    "prismjs": "^1.30.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.12",
    "@angular/cli": "17.3.12",
    "@angular/compiler-cli": "17.3.12",
    "@playwright/test": "^1.57.0",
    "@types/canvas-confetti": "^1.9.0",
    "@types/jasmine": "~5.1.0",
    "@types/prismjs": "^1.26.5",
    "angular-cli-ghpages": "^2.0.3",
    "autoprefixer": "^10.4.21",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "monaco-editor": "^0.43.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.2.2"
  }
}
