  "openFile": "/src/App.vue",
  "files": {
    "package.json": "{\"name\":\"vue-chessboard-click-highlight-solution\",\"version\":\"0.1.0\",\"private\":true,\"dependencies\":{\"vue\":\"3.x\"},\"scripts\":{\"serve\":\"vue-cli-service serve\"}}\n",
    "public/index.html": "<!doctype html><html lang=\"en\"><head><meta charset=\"utf-8\"><meta name=\"viewport\" content=\"width=device-width,initial-scale=1\"><title>Vue SDK • Chessboard Click Highlight (solution)</title></head><body><div id=\"app\"></div></body></html>",
    "src/main.ts": "import { createApp } from 'vue';\nimport App from './App.vue';\nimport './App.css';\n\ncreateApp(App).mount('#app');\n",
    "src/App.vue": "<template>\n  <main class=\"page\">\n    <section class=\"card\">\n      <h1>Chessboard Click / Highlight</h1>\n      <p class=\"subtitle\">\n        Build an N×N chessboard where clicking one cell highlights it and clears previous selection.\n      </p>\n\n      <label class=\"size\" for=\"board-size\">\n        Board size (N)\n        <input\n          id=\"board-size\"\n          type=\"number\"\n          :min=\"MIN_SIZE\"\n          :max=\"MAX_SIZE\"\n          :value=\"size\"\n          @input=\"onSizeInput(String(($event.target && $event.target.value) || ''))\"\n        />\n      </label>\n\n      <p class=\"status\" aria-live=\"polite\">{{ selectedLabel }}</p>\n\n      <div\n        class=\"board\"\n        role=\"grid\"\n        aria-label=\"interactive chessboard\"\n        :style=\"{ gridTemplateColumns: 'repeat(' + size + ', minmax(0, 1fr))' }\"\n      >\n        <template v-for=\"row in rows\" :key=\"'row-' + row\">\n          <button\n            v-for=\"col in cols\"\n            :key=\"cellKey(row, col)\"\n            type=\"button\"\n            role=\"gridcell\"\n            :class=\"cellClass(row, col)\"\n            @click=\"onCellClick(row, col)\"\n            :aria-label=\"'Row ' + (row + 1) + ' Column ' + (col + 1)\"\n            :aria-pressed=\"selectedKey === cellKey(row, col)\"\n          ></button>\n        </template>\n      </div>\n    </section>\n  </main>\n</template>\n\n<script setup lang=\"ts\">\nimport { computed, ref } from 'vue';\n\nconst MIN_SIZE = 2;\nconst MAX_SIZE = 20;\n\nconst size = ref(8);\nconst selectedKey = ref<string | null>(null);\n\nfunction onSizeInput(raw: string) {\n  const next = Number(raw);\n  if (!Number.isFinite(next)) return;\n  const clamped = Math.max(MIN_SIZE, Math.min(MAX_SIZE, Math.trunc(next)));\n  size.value = clamped;\n  selectedKey.value = null;\n}\n\nfunction onCellClick(row: number, col: number) {\n  selectedKey.value = cellKey(row, col);\n}\n\nconst rows = computed(() => Array.from({ length: size.value }, (_, index) => index));\nconst cols = computed(() => Array.from({ length: size.value }, (_, index) => index));\n\nconst selectedLabel = computed(() => {\n  if (!selectedKey.value) return 'Selected: none';\n  const [row, col] = selectedKey.value.split('-').map(Number);\n  return 'Selected: row ' + String(row + 1) + ', column ' + String(col + 1);\n});\n\nfunction cellKey(row: number, col: number): string {\n  return String(row) + '-' + String(col);\n}\n\nfunction cellClass(row: number, col: number): string {\n  const dark = (row + col) % 2 === 1;\n  const active = selectedKey.value === cellKey(row, col);\n  return 'cell ' + (dark ? 'cell--dark' : 'cell--light') + (active ? ' cell--active' : '');\n}\n</script>\n",
    "src/App.css": ":root{color-scheme:dark;}\nhtml,body{margin:0;height:100%;background:#0a0a0a;}\nbody{font-family:system-ui,-apple-system,Segoe UI,Roboto,Helvetica,Arial,sans-serif;}\n.page{min-height:100vh;display:grid;place-items:center;padding:24px;background:#0a0a0a;color:#e5e7eb;}\n.card{width:min(760px,100%);background:#0b1020;border:1px solid #1f2937;border-radius:14px;padding:18px;display:grid;gap:14px;}\nh1{margin:0;font-size:20px;}\n.subtitle{margin:0;color:#9ca3af;font-size:13px;}\n.size{display:inline-grid;gap:6px;font-size:12px;color:#9ca3af;width:max-content;}\n.size input{width:90px;background:#0a0f1e;color:#e5e7eb;border:1px solid #374151;border-radius:10px;padding:8px 10px;}\n.status{margin:0;padding:8px 10px;border:1px solid #1f2937;border-radius:10px;background:#0a0f1e;font-size:13px;}\n.board{width:min(560px,100%);display:grid;border:1px solid #374151;border-radius:12px;overflow:hidden;}\n.cell{aspect-ratio:1/1;border:none;cursor:pointer;outline:none;box-shadow:inset 0 0 0 1px rgba(15,23,42,.15);transition:box-shadow 120ms ease;}\n.cell--light{background:#e5e7eb;}\n.cell--dark{background:#334155;}\n.cell:hover{box-shadow:inset 0 0 0 2px rgba(14,165,233,.65);}\n.cell--active{box-shadow:inset 0 0 0 3px #facc15 !important;}\n.cell:focus-visible{box-shadow:inset 0 0 0 3px #a3e635 !important;}\n",
    "tsconfig.json": "{\n  \"compilerOptions\": {\n    \"target\": \"ES2020\",\n    \"module\": \"ESNext\",\n    \"moduleResolution\": \"Node\",\n    \"strict\": true,\n    \"allowJs\": true\n  }\n}\n"
  }
}
