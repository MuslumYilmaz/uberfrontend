    "files": {
        "package.json": "{\n  \"name\": \"vue-transfer-list-solution\",\n  \"version\": \"0.1.0\",\n  \"private\": true,\n  \"dependencies\": {\n    \"vue\": \"3.x\"\n  },\n  \"scripts\": {\n    \"serve\": \"vue-cli-service serve\"\n  }\n}\n",
        "public/index.html": "<!doctype html><html lang=\"en\"><head><meta charset=\"utf-8\"><meta name=\"viewport\" content=\"width=device-width,initial-scale=1\"><title>Vue SDK • Transfer List (solution)</title></head><body><div id=\"app\"></div></body></html>",
        "src/main.ts": "import { createApp } from 'vue';\nimport App from './App.vue';\nimport './App.css';\n\ncreateApp(App).mount('#app');\n",
        "src/App.vue": "<template>\n  <main class=\"page\">\n    <section class=\"card\">\n      <h1 class=\"title\">Transfer List</h1>\n      <p class=\"subtitle\">Select items and move them between lists.</p>\n\n      <div class=\"layout\">\n        <section class=\"listCard\">\n          <div class=\"listTitle\">Top</div>\n          <ul class=\"list\" role=\"list\">\n            <li v-for=\"item in topItems\" :key=\"item.id\" class=\"row\">\n              <label class=\"rowLabel\">\n                <input\n                  type=\"checkbox\"\n                  :checked=\"topSelected.has(item.id)\"\n                  @change=\"onTopChange(item.id, $event)\"\n                />\n                <span>{{ item.label }}</span>\n              </label>\n            </li>\n            <li v-if=\"topItems.length === 0\" class=\"empty\">Empty</li>\n          </ul>\n        </section>\n\n        <section class=\"controls\">\n          <button class=\"btn\" @click=\"moveDown\" :disabled=\"!canMoveDown\" aria-label=\"move down\">\n            ↓\n          </button>\n          <button class=\"btn\" @click=\"moveUp\" :disabled=\"!canMoveUp\" aria-label=\"move up\">\n            ↑\n          </button>\n        </section>\n\n        <section class=\"listCard\">\n          <div class=\"listTitle\">Bottom</div>\n          <ul class=\"list\" role=\"list\">\n            <li v-for=\"item in bottomItems\" :key=\"item.id\" class=\"row\">\n              <label class=\"rowLabel\">\n                <input\n                  type=\"checkbox\"\n                  :checked=\"bottomSelected.has(item.id)\"\n                  @change=\"onBottomChange(item.id, $event)\"\n                />\n                <span>{{ item.label }}</span>\n              </label>\n            </li>\n            <li v-if=\"bottomItems.length === 0\" class=\"empty\">Empty</li>\n          </ul>\n        </section>\n      </div>\n\n      <p class=\"hint\">Tip: selections are per-list. Move clears selection of moved items.</p>\n    </section>\n  </main>\n</template>\n\n<script setup lang=\"ts\">\nimport { computed, ref } from 'vue';\n\ntype Item = { id: string; label: string };\n\nconst INITIAL_TOP: Item[] = [\n  { id: 'a', label: 'Alpha' },\n  { id: 'b', label: 'Beta' },\n  { id: 'c', label: 'Gamma' },\n  { id: 'd', label: 'Delta' },\n  { id: 'e', label: 'Epsilon' }\n];\n\nconst INITIAL_BOTTOM: Item[] = [\n  { id: 'x', label: 'Xenon' },\n  { id: 'y', label: 'Yttrium' },\n  { id: 'z', label: 'Zinc' }\n];\n\nconst topItems = ref<Item[]>(INITIAL_TOP);\nconst bottomItems = ref<Item[]>(INITIAL_BOTTOM);\n\nconst topSelected = ref<Set<string>>(new Set<string>());\nconst bottomSelected = ref<Set<string>>(new Set<string>());\n\nconst canMoveDown = computed(() => topSelected.value.size > 0);\nconst canMoveUp = computed(() => bottomSelected.value.size > 0);\n\nconst onTopChange = (id: string, e: Event) => {\n  const checked = (e.currentTarget as HTMLInputElement | null)?.checked ?? false;\n  toggleTop(id, checked);\n};\n\nconst onBottomChange = (id: string, e: Event) => {\n  const checked = (e.currentTarget as HTMLInputElement | null)?.checked ?? false;\n  toggleBottom(id, checked);\n};\n\nconst toggleTop = (id: string, checked: boolean) => {\n  const next = new Set(topSelected.value);\n  if (checked) next.add(id);\n  else next.delete(id);\n  topSelected.value = next;\n};\n\nconst toggleBottom = (id: string, checked: boolean) => {\n  const next = new Set(bottomSelected.value);\n  if (checked) next.add(id);\n  else next.delete(id);\n  bottomSelected.value = next;\n};\n\nconst moveDown = () => {\n  if (!canMoveDown.value) return;\n\n  const moved: Item[] = [];\n  const remaining: Item[] = [];\n\n  for (const it of topItems.value) {\n    if (topSelected.value.has(it.id)) moved.push(it);\n    else remaining.push(it);\n  }\n\n  topItems.value = remaining;\n  bottomItems.value = [...bottomItems.value, ...moved];\n  topSelected.value = new Set<string>();\n};\n\nconst moveUp = () => {\n  if (!canMoveUp.value) return;\n\n  const moved: Item[] = [];\n  const remaining: Item[] = [];\n\n  for (const it of bottomItems.value) {\n    if (bottomSelected.value.has(it.id)) moved.push(it);\n    else remaining.push(it);\n  }\n\n  bottomItems.value = remaining;\n  topItems.value = [...topItems.value, ...moved];\n  bottomSelected.value = new Set<string>();\n};\n</script>\n",
        "src/App.css": ":root{color-scheme:dark;}html,body{margin:0;height:100%;background:#0a0a0a;}body{font-family:system-ui,-apple-system,Segoe UI,Roboto,Helvetica,Arial,sans-serif;}\n.page{min-height:100vh;display:grid;place-items:center;padding:24px;background:#0a0a0a;color:#e5e7eb;}\n.card{width:100%;max-width:780px;background:#0b1020;border:1px solid #1f2937;border-radius:14px;padding:18px;display:grid;gap:14px;}\n.title{margin:0;font-size:18px;font-weight:800;}\n.subtitle{margin:0;color:#9ca3af;font-size:12px;}\n.layout{display:grid;grid-template-columns:1fr auto 1fr;gap:14px;align-items:start;}\n.listCard{background:#0a0f1e;border:1px solid #1f2937;border-radius:12px;padding:12px;}\n.listTitle{font-size:12px;color:#9ca3af;margin-bottom:8px;}\n.list{list-style:none;margin:0;padding:0;display:grid;gap:6px;}\n.row{padding:8px 10px;border:1px solid rgba(31,41,55,.5);border-radius:10px;}\n.rowLabel{display:flex;gap:10px;align-items:center;cursor:pointer;}\n.rowLabel input{accent-color:#a3e635;}\n.controls{display:grid;gap:10px;place-items:center;padding-top:22px;}\n.btn{width:44px;height:44px;border-radius:12px;border:1px solid #374151;background:#0a0f1e;color:#e5e7eb;font-weight:900;cursor:pointer;}\n.btn:disabled{opacity:.5;cursor:not-allowed;}\n.empty{padding:8px 10px;color:#9ca3af;}\n.hint{margin:0;color:#9ca3af;font-size:12px;}\n@media (max-width:720px){.layout{grid-template-columns:1fr;}.controls{grid-template-columns:1fr 1fr;justify-content:center;padding-top:0;}}\n",
        "tsconfig.json": "{\n  \"compilerOptions\": {\n    \"target\": \"ES2020\",\n    \"module\": \"ESNext\",\n    \"moduleResolution\": \"Node\",\n    \"strict\": true,\n    \"allowJs\": true\n  }\n}\n"
    }
}