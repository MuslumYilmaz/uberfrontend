{
    "dependencies": {
        "vue": "3.x"
    },
    "openFile": "/src/App.vue",
    "files": {
        "package.json": "{\n  \"name\": \"vue-pagination-table-solution\",\n  \"version\": \"0.1.0\",\n  \"private\": true,\n  \"dependencies\": {\n    \"vue\": \"3.x\"\n  },\n  \"scripts\": {\n    \"serve\": \"vue-cli-service serve\"\n  }\n}\n",
        "public/index.html": "<!doctype html><html lang=\"en\"><head><meta charset=\"utf-8\"><meta name=\"viewport\" content=\"width=device-width,initial-scale=1\"><title>Vue SDK â€¢ Paginated Table (solution)</title></head><body><div id=\"app\"></div></body></html>",
        "src/main.ts": "import { createApp } from 'vue';\nimport App from './App.vue';\nimport './App.css';\n\ncreateApp(App).mount('#app');\n",
        "src/App.vue": "<template>\n  <main class=\"main\">\n    <section class=\"section\">\n      <header class=\"header\">\n        <div>\n          <h1>Users (Solution)</h1>\n          <p class=\"subtitle\">Simple paginated table (5 per page)</p>\n        </div>\n        <div class=\"meta\">Total users: {{ USERS.length }}</div>\n      </header>\n\n      <div class=\"panel\">\n        <table class=\"table\" aria-label=\"Users table\">\n          <thead>\n            <tr>\n              <th>ID</th>\n              <th>Name</th>\n              <th>Email</th>\n              <th>Role</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"user in pagedUsers\" :key=\"user.id\">\n              <td>{{ user.id }}</td>\n              <td>{{ user.name }}</td>\n              <td>{{ user.email }}</td>\n              <td>{{ user.role }}</td>\n            </tr>\n          </tbody>\n        </table>\n\n        <footer class=\"footer\">\n          <button\n            type=\"button\"\n            class=\"ghost\"\n            @click=\"goPrevious\"\n            :disabled=\"isFirstPage\"\n          >\n            Previous\n          </button>\n          <span class=\"page-info\">\n            Page {{ currentPage + 1 }} of {{ totalPages }}\n          </span>\n          <button\n            type=\"button\"\n            @click=\"goNext\"\n            :disabled=\"isLastPage\"\n          >\n            Next\n          </button>\n        </footer>\n      </div>\n    </section>\n  </main>\n</template>\n\n<script setup lang=\"ts\">\nimport { ref, computed } from 'vue';\n\ntype User = {\n  id: number;\n  name: string;\n  email: string;\n  role: string;\n};\n\nconst USERS: User[] = [\n  { id: 1, name: 'Alice Johnson', email: 'alice@example.com', role: 'Admin' },\n  { id: 2, name: 'Bob Smith', email: 'bob@example.com', role: 'Editor' },\n  { id: 3, name: 'Carla Perez', email: 'carla@example.com', role: 'Viewer' },\n  { id: 4, name: 'David Kim', email: 'david@example.com', role: 'Editor' },\n  { id: 5, name: 'Emma Brown', email: 'emma@example.com', role: 'Viewer' },\n  { id: 6, name: 'Frank Wu', email: 'frank@example.com', role: 'Admin' },\n  { id: 7, name: 'Grace Lee', email: 'grace@example.com', role: 'Viewer' },\n  { id: 8, name: 'Henry Ford', email: 'henry@example.com', role: 'Editor' },\n  { id: 9, name: 'Ivy Stone', email: 'ivy@example.com', role: 'Viewer' },\n  { id: 10, name: 'Jack Ryan', email: 'jack@example.com', role: 'Viewer' },\n  { id: 11, name: 'Karen Diaz', email: 'karen@example.com', role: 'Editor' },\n  { id: 12, name: 'Leo Walker', email: 'leo@example.com', role: 'Viewer' },\n  { id: 13, name: 'Mia Clark', email: 'mia@example.com', role: 'Admin' },\n  { id: 14, name: 'Noah Hall', email: 'noah@example.com', role: 'Viewer' },\n  { id: 15, name: 'Olivia King', email: 'olivia@example.com', role: 'Editor' },\n  { id: 16, name: 'Paul Green', email: 'paul@example.com', role: 'Viewer' },\n  { id: 17, name: 'Quinn Ross', email: 'quinn@example.com', role: 'Viewer' },\n  { id: 18, name: 'Rita Long', email: 'rita@example.com', role: 'Editor' },\n  { id: 19, name: 'Sam Young', email: 'sam@example.com', role: 'Viewer' },\n  { id: 20, name: 'Tina Fox', email: 'tina@example.com', role: 'Viewer' }\n];\n\nconst PAGE_SIZE = 5;\n\nconst currentPage = ref(0);\n\nconst totalPages = computed(() => {\n  return Math.ceil(USERS.length / PAGE_SIZE) || 1;\n});\n\nconst pagedUsers = computed(() => {\n  const startIndex = currentPage.value * PAGE_SIZE;\n  const endIndex = startIndex + PAGE_SIZE;\n  return USERS.slice(startIndex, endIndex);\n});\n\nconst isFirstPage = computed(() => currentPage.value === 0);\n\nconst isLastPage = computed(() => currentPage.value >= totalPages.value - 1);\n\nconst goPrevious = () => {\n  if (currentPage.value > 0) {\n    currentPage.value -= 1;\n  }\n};\n\nconst goNext = () => {\n  if (currentPage.value < totalPages.value - 1) {\n    currentPage.value += 1;\n  }\n};\n</script>\n",
        "src/App.css": ":root { color-scheme: dark; }\nhtml, body { margin: 0; height: 100%; background:#020617; font-family: system-ui, -apple-system, Segoe UI, Roboto, Helvetica, Arial, sans-serif; }\n\n.main { min-height:100vh; background:#020617; color:#e5e7eb; display:grid; place-items:center; padding:24px; }\n.section { width:100%; max-width:720px; background:#020617; border-radius:16px; border:1px solid #1f2937; padding:20px; box-sizing:border-box; display:grid; gap:12px; }\n\n.header { display:flex; align-items:flex-end; justify-content:space-between; gap:12px; }\n.header h1 { margin:0; font-size:20px; font-weight:700; }\n.subtitle { margin:2px 0 0; font-size:12px; color:#9ca3af; }\n.meta { font-size:12px; color:#9ca3af; }\n\n.panel { background:#020617; border-radius:12px; border:1px solid #1f2937; overflow:hidden; }\n\n.table { width:100%; border-collapse:collapse; font-size:13px; }\n.table thead { background:#0b1020; }\n.table th, .table td { padding:8px 10px; border-bottom:1px solid #111827; text-align:left; }\n.table tbody tr:nth-child(even) { background:#020617; }\n\n.footer { display:flex; align-items:center; justify-content:space-between; padding:10px 12px; gap:8px; }\n.page-info { font-size:12px; color:#9ca3af; }\n\nbutton { border:none; border-radius:999px; padding:6px 14px; font-size:13px; font-weight:600; cursor:pointer; background:#22c55e; color:#021210; }\nbutton.ghost { background:transparent; color:#e5e7eb; border:1px solid #374151; }\nbutton[disabled] { opacity:0.5; cursor:not-allowed; }\n",
        "tsconfig.json": "{\n  \"compilerOptions\": {\n    \"target\": \"ES2020\",\n    \"module\": \"ESNext\",\n    \"moduleResolution\": \"Node\",\n    \"strict\": true,\n    \"allowJs\": true\n  }\n}\n"
    }
}